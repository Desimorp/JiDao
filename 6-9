#include<stdio.h>

void printFactor( int, int ); 

int main()
{
    int a,b,i ;
    
    scanf( "%d%d", &a, &b );
    for( i = a ; i <= b ; i++ )
        printFactor( i , 1 ) ;    
    
    return 0;
}

/* 请在这里填写答案 */
void printFactor( int n, int flag ){//n >= flag
    if(flag == 1){
        printf("%d=", n);//n==1出现死循环的处理（余数判定无法更新变量）
        if(n==1){
            printf("1\n");
            return;
        }
        printFactor(n / flag, flag + 1);
    }
    else if(n == flag){
        printf("%d\n",flag);
        return; 
    }
    else if(n % flag == 0){
        printf("%d*",flag);
        printFactor(n / flag, flag);
    }
    else if(n % flag != 0){
        printFactor(n, flag + 1);
    }
}
