#include<stdio.h>
#include<math.h>

int ifPrime(int x);

int main()
{
	int n;
	scanf("%d", &n);
	for (int i = 0; i < n; i++)
	{
		int s;
		int sum = 0;
		scanf("%d", &s);
		while(s!=-1)
		{
			sum += ifPrime(s);
			scanf("%d", &s);
		}
		printf("%d\n", sum);
	}
	return 0;
}

int ifPrime(int x)
{
	for (int i = 2; i <= (int)sqrt(x); i++)
	{
		if (x % i == 0)
			return 0;
	}
	return 1;
}
