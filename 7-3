#include<stdio.h>

int main(){
	int Num[200] = { 0 };
	int New[200] = { 0 };
	int n;
	scanf("%d", &n);
	for(int i = 0; i < n; i++){
		scanf("%d", &Num[i]);
	}
	int j = 0;
	for(int i = 0; i < n; i++){
		if(Num[i] % 2 == 0)	
			New[j++] = Num[i];
	}
	for(int i = 0; i < n; i++){
		if(Num[i] % 2!= 0)
			New[j++] = Num[i];
	}
	for(int i = 0; i < j; i++){
		if(i == j - 1)
			printf("%d\n", New[i]);
		else 
			printf("%d ", New[i]);
	}
	return 0;
}
