#include<stdio.h>
int main() {
	int x;
	scanf("%d", &x);
	if (x < 1 || x>10000)
		printf("%d out of range", x);
	else {
		int y;
		y = x * x;
		while (x != 0) {
			if (x % 10 == y % 10)
				;
			else
				break;
			x /= 10;
			y /= 10;
		}
		if (x == 0)
			printf("Yes");
		else
			printf("No");
	}
	return 0;
}
