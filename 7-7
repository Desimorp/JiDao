#include<stdio.h>

int Init(int *Array){
	int ele = 0;
	scanf("%d", &ele);
	int i = 0;
	while(ele!= -1){
		Array[i] = ele;
		scanf("%d", &ele);
		i++;
	}
	return i;//size
}

int Compare(int *ListA,int *ListB,int sizeA,int sizeB){
	int result = 0;
	for(int i = 0; i < sizeA; i++){
		if(ListA[i] == ListB[0]){
			if(sizeA - i >= sizeB){
				result = 1;
				for(int j = 1; j < sizeB; j++){
					if(ListB[j] != ListA[i + j]){
						result = 0;
						break;
					}
				}
				if(result)
					break;
			}
		}
	}
	return result;
}

void Print(int result){
	if(result)
		printf("ListB is the sub sequence of ListA.\n");
	else
		printf("ListB is not the sub sequence of ListA.\n");
}

int main(){
	int A[1000] = { 0 };
	int B[1000] = { 0 };
	Print(Compare(A,B,Init(A),Init(B));
	return 0;
}
