#include<stdio.h>
#include<stdlib.h>

int main(){
    int n, m;
    scanf("%d %d", &n, &m);
    int**Matrix;
    Matrix = (int**)malloc(n * sizeof(int*));
    for(int i = 0; i < n; i++){
        Matrix[i] = (int*)malloc(m * sizeof(int));
    }
    for(int i = 0; i < n; i++){
        for(int j = 0; j < m; j++){
            scanf("%d", &Matrix[i][j]);
            printf("%d", 10 * (0 - Matrix[i][j]));
            if(j!= m - 1){
                printf(" ");
            }
            else{
                printf("\n");
            }
        }
    }
    for(int i = 0; i < n; i++){
        free(Matrix[i]);
    }
    free(Matrix);
    return 0;
}
