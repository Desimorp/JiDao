#include<stdio.h>
#define MAX 100

void Input(int Matrix[][MAX],int n){
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            scanf("%d",&Matrix[i][j]);
        }
    }
}

void Transpose(int Matrix[][MAX],int n){
    int Order[MAX];
    int Average[MAX] = {0};
    for(int i = 0; i < n; i++)
        Order[i]=i;
    for(int i = 0; i < n; i++){
        int sum = 0;
        for(int j = 0; j < n; j++){
            sum += Matrix[i][j];
        }
        Average[i] = sum;
    }
    for(int i = 0; i < n - 1; i++){//冒泡排序
        for(int j = 0; j < n - 1 - i; j++){
            if(Average[j] > Average[j+1]){
                int temp = Order[j];
                Order[j] = Order[j+1];
                Order[j+1] = temp;
                temp = Average[j];
                Average[j] = Average[j+1];
                Average[j+1] = temp;
            }
        }
    }
    for(int i = 0; i < n; i++){
        for(int j = 0; j < n; j++){
            printf("%d",Matrix[Order[i]][j]);
            if(j!= n - 1)
                printf(" ");
            else
                printf("\n");
        }
    }
}
int main(){
    int n;
    scanf("%d",&n);
    int Matrix[MAX][MAX];
    Input(Matrix,n);
    Transpose(Matrix,n);
    return 0;
}
