#include<stdio.h>

int Find(int x, int*a, int n) {
	for (int i = 0; i < n; i++) {
		if (x == a[i]) {
			return i;
		}
	}
	return -1;
}

void Print(int x, int*a, int n) {	
    if((n == 1)||(x == -1))
        printf("NULL");
	else if(x == 0)
        printf("%d", a[x + 1]);
    else if(x == n - 1)
        printf("%d", a[x - 1]);
    else 
        printf("%d %d", a[x - 1], a[x + 1]);
	printf("\n");
}

int main() {
	int a[1000] = { 0 };
	int n;
	scanf("%d", &n);
	for (int i = 0; i < n; i++) {
		scanf("%d", &a[i]);
	}
	int m;
	scanf("%d", &m);
	for (int i = 0; i < m; i++) {
		int tem = 0;
		scanf("%d", &tem);
		Print(Find(tem, a, n), a, n);
	}
	return 0;
}
