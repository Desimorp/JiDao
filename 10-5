#include<stdio.h>

int getString( char * source , char *strPtr[] ) ;

int main()
{
    char    str[100005];
    char    *strPtr[1005]={0};
    int        i, num ;
    
    gets(str);
    num = getString( str , strPtr ) ;
    for( i = 0 ; i < num ; i++ )
        puts(strPtr[i]);
    
    return 0;    
}

/* 请在这里填写答案 */
int getString( char * source, char *strPtr[] ){
    int num = 1;
    int temp = 0;
    if(source[0] != ' '){
        strPtr[0] = &source[0];

    }
    else{
        int i = 0;
        while(source[i] == ' '){
            i++;
        }
        strPtr[0] = &source[i];
        temp = i;
    }
    for(int i = temp + 1; source[i] != '\0'; i++){
        if(source[i] == ' '){
            source[i] = '\0';
            if(source[i + 1] != '\0' && source[i + 1] != ' '){
                strPtr[num] = &source[i + 1];
                num++;
            }
        }
    }
    return num;
}
