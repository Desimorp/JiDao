#include<stdio.h>

int main()
{
	int n;
	scanf("%d", &n);
	for (int i = 0; i < n; i++)
	{
		int m, k =0;
		scanf("%d", &m);
		for (int j = 0; j < m; j++)
		{
			int n;
			scanf("%d", &n);
			k += n;
		}
		if (k >= 400)
			printf("%d\n", k - 160);
		else if (k >= 300)
			printf("%d\n", k - 110);
		else if (k >= 200)
			printf("%d\n", k - 70);
		else if (k >= 100)
			printf("%d\n", k - 30);
		else
			printf("%d\n", k);
	}
	return 0;
}
