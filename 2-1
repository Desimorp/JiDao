#include<stdio.h>

int main() {
	int a, b, max, min;
	scanf("%d %d", &a, &b);
	if (a > b) {
		printf("The larger number is %d, the smaller number is %d.", a, b);
		max = a;
		min = b;
	}
	else if(a < b){
		printf("The larger number is %d, the smaller number is %d.", b, a);
		max = b;
		min = a;
	}
	else {
		printf("The two numbers are equal.");
		max = a;
		min = a;
	}
	return 0;
}
