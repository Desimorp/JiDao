#include<stdio.h>

int main(){
	int a[10] = { 0 };
	int b[10] = { 0 };
	int n;
	scanf("%d", &n);
	for(int i = 0; i < 2 * n; i++){
		if(i < n)
			scanf("%d", &a[i]);
		else
			scanf("%d", &b[i - n]);
	}
	int sum = 0;
	for(int i = 0; i < n; i++){
		sum += a[i] * b[i];
	}
	printf("%d\n", sum);
	return 0;
}
