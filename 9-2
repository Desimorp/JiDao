#include<stdio.h>
#include<string.h>

int Mul(int *a,char *b){
	int sum = 0;
	for(int i = 0; i < 17; i++){
		sum += a[i] * (b[i] - '0');
	}
	return sum;
}

int main(){
	int n;
	int Coe[17] = {7,9,10,5,8,4,2,1,6,3,7,9,10,5,8,4,2};
	char result[11] = { '1', '0', 'X', '9', '8', '7', '6', '5', '4', '3', '2' };
	scanf("%d",&n);
	for(int i = 0; i < n; i++){
		int sum = 0;
		char Card[18] = {0};
		scanf("%s",Card);
		//getchar();
		sum = Mul(Coe,Card);
		
		if(result[sum % 11] == Card[17])
			printf("right\n");
		else
			printf("wrong\n");
	}
	return 0;
}
