#include <stdio.h>
#include <stdlib.h>
int main(){
    int n;
    scanf("%d", &n);
    int*array;
    array = (int*)malloc(n * sizeof(int));
    for(int i = 0; i < n; i++){
        scanf("%d", &array[i]);
        printf("%d", array[i] = 10 * (0 - array[i]));
        if(i != n - 1)
            printf(" ");
        else
            printf("\n");
    }
    free(array);
    return 0;
}
