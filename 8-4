#include<stdio.h>
#define MAX 10

void Input(int Matrix[MAX][MAX],int m,int n){
	for(int i = 0;i < m;i++){
		for(int j = 0;j < n;j++){
			scanf("%d",&Matrix[i][j]);
		}
	}
}

void Calculate(int Matrix[MAX][MAX],int m,int n,int x,int y){
	int sum = 0;
	int pos_x = --x;
	int pos_y = --y;
	for(int i = pos_x - 1; i < pos_x + 2; i++){
		for(int j = pos_y - 1; j < pos_y + 2; j++){
			if(i >= 0 && i < m && j >= 0 && j < n)
					sum += Matrix[i][j];
		}
	}
	printf("%d",sum - Matrix[pos_x][pos_y]);
}
	
int main(){
	int m,n;
	scanf("%d%d",&m,&n);
	int Matrix[MAX][MAX] = {0};
	Input(Matrix,m,n);
	int x, y;
	scanf("%d %d", &x, &y);
	Calculate(Matrix,m,n,x,y);
	return 0;
}
