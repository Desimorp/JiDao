#include <stdio.h>

int findMax(int n) ;

int main()
{   
    int n ; 
    
    scanf("%d", &n); 
    printf("%d\n" , findMax( n ) ) ; 
    
    return 0;
}

/* 请在这里填写答案 */
int findMax(int n){
    int t;
    scanf("%d", &t);
    if(n == 1){
        return t;
    }
    else{
        int x = findMax(n - 1);
        if(t >= x){
            return t;
        }
        else{
            return x;
        }
    }
}
