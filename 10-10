#include<stdio.h>
#include<stdlib.h>
#include<string.h>

int main()
{
    int n = 0, i = 0, j = 0;
    scanf("%d", &n);
    getchar();
 
    char* arr;
    arr = (char*)malloc(sizeof(char) * n * 1000);
 
    char* ptr[100000];
    // ptr=(char *)malloc(sizeof(char *)*n);
    ptr[0] = arr;
 
 
    char ch[1000] = { 0 };
    for (i = 0; i < n; i++)
    {
        gets(ch);
        strcpy(ptr[i], ch);
        ptr[i + 1] = ptr[i] + strlen(ch) + 1;
    }
    char* f;
    for (i = 0; i < n; i++)
    {
        for (j = 0; j < n - i - 1; j++)
        {
            if (strcmp(ptr[j], ptr[j + 1]) > 0)
            {
                f = ptr[j];
                ptr[j] = ptr[j + 1];
                ptr[j + 1] = f;
            }
        }
    }
    for (i = 0; i < n; i++)
        puts(ptr[i]);
    return 0;
}
